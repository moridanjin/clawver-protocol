{
  "framework": null,
  "buildCommand": "npm run build",
  "outputDirectory": "public",
  "rewrites": [
    { "source": "/health", "destination": "/api" },
    { "source": "/agents/:path*", "destination": "/api" },
    { "source": "/skills/:path*", "destination": "/api" },
    { "source": "/execute/:path*", "destination": "/api" },
    { "source": "/executions/:path*", "destination": "/api" },
    { "source": "/contracts/:path*", "destination": "/api" }
  ],
  "functions": {
    "api/index.ts": {
      "maxDuration": 30
    }
  }
}
